{
    "name": "copy-request",
    "version": "1.0",
    "minimum_chrome_version": "18.0",
    "devtools_page": "index.html",
    "manifest_version": 2,
    "permissions": [
        "clipboardWrite",
        "storage",
        {
            "fileSystem": [
                "write",
                "retainEntries",
                "directory",
                "requestFileSystem"
            ]
        },
        "downloads"
    ]
}